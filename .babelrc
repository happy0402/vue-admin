{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
        //windowsXP最后浏览器版本号
        "targets": {
          "chrome": "49",
          "ie": "8",
          "firefox": "52"
        }
//        "useBuiltIns": "usage",
//        "corejs": 2
      }
    ]
  ],
  "plugins": [
    [
      "component",
      {
        "libraryName": "element-ui",
        "styleLibraryName": "theme-chalk" //"styleLibraryName": "~theme"
      }
    ],
    [
      "import",
      {
        "libraryName": "view-design",
        "libraryDirectory": "src/components"
      }
    ]
  ],
  "comments": false
}